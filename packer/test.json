{
    "builders": [{
        "type": "vagrant",
        "source_path": "ubuntu/xenial64",
        "provider": "virtualbox",
        "communicator": "ssh",
        "box_name": "test",
        "add_force": true
    }],
    "provisioners": [{
        "type": "ansible",
        "user": "root",
        "playbook_file": "playbook.yml"
    }],

    "post-processors": [{
        "type": "vagrant",
        "compression_level": 1,
        "override": {
          "vmware": {
            "compression_level": 0
          }
        }
      }]
}